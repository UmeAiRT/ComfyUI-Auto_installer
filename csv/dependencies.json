{
  "tools": {
    "python": {
      "version": "3.12.9",
      "url": "https://www.python.org/ftp/python/3.12.9/python-3.12.9-amd64.exe",
      "arguments": "/quiet InstallAllUsers=1 PrependPath=1"
    },
    "git": {
      "version": "2.41.0.3",
      "url": "https://github.com/git-for-windows/git/releases/download/v2.41.0.windows.3/Git-2.41.0.3-64-bit.exe",
      "arguments": "/VERYSILENT"
    },
    "seven_zip": {
      "version": "22.01",
      "url": "https://www.7-zip.org/a/7z2201-x64.exe",
      "arguments": "/S",
      "path": "C:\\Program Files\\7-Zip\\7z.exe"
    },
    "aria2": {
      "version": "1.36.0",
      "url": "https://github.com/aria2/aria2/releases/download/release-1.36.0/aria2-1.36.0-win-64bit-build1.zip",
      "install_path": "C:\\Tools\\aria2"
    },
    "ninja": {
      "version": "1.13.1",
      "url": "https://github.com/ninja-build/ninja/releases/download/v1.13.1/ninja-win.zip",
      "install_path": "C:\\Tools\\ninja"
    },
    "vs_build_tools": {
      "url": "https://aka.ms/vs/17/release/vs_BuildTools.exe",
      "arguments": "--quiet --wait --norestart --add Microsoft.VisualStudio.Component.VC.Tools.x86.x64 --add Microsoft.VisualStudio.Component.VC.CMake.Project --add Microsoft.VisualStudio.Component.Windows11SDK.26100"
    }
  },
  "repositories": {
    "comfy_ui": "https://github.com/comfyanonymous/ComfyUI.git",
    "workflows": "https://github.com/UmeAiRT/ComfyUI-Workflows"
  },
  "pip_packages": {
    "upgrade": [
      "pip",
      "wheel"
    ],
    "torch": {
      "packages": "torch==2.8.0.dev20250627 torchvision torchaudio",
      "index_url": "https://download.pytorch.org/whl/nightly/cu129"
    },
    "comfyui_requirements": "requirements.txt",
    "wheels": [
      {
        "name": "nunchaku",
        "url": "https://github.com/nunchaku-tech/nunchaku/releases/download/v0.3.1/nunchaku-0.3.1+torch2.8-cp312-cp312-win_amd64.whl"
      },
      {
        "name": "insightface",
        "url": "https://github.com/UmeAiRT/ComfyUI-Auto_installer/raw/refs/heads/main/whl/insightface-0.7.3-cp312-cp312-win_amd64.whl"
      },
      {
        "name": "triton",
        "url": "https://github.com/UmeAiRT/ComfyUI-Auto_installer/raw/refs/heads/main/whl/triton-3.3.0-py3-none-any.whl"
      }
    ],
    "standard": [
      "facexlib",
      "cython",
      "onnxruntime",
      "onnxruntime-gpu",
      "triton-windows"
    ],
    "pinned": [
      "numpy==1.26.4",
      "pandas",
      "transformers==4.49.0"
    ],
    "git_repos": [
      {
        "name": "xformers",
        "url": "https://github.com/facebookresearch/xformers.git",
        "commit": "4834B4152B3A449B551AAB5F87723737320D5D4E"
      },
      {
        "name": "SageAttention",
        "url": "https://github.com/thu-ml/SageAttention",
        "commit": "3a33598686e54555b774435ceb070444369a19e5"
      },
      {
        "name": "apex",
        "url": "https://github.com/NVIDIA/apex.git",
        "commit": "f4f08006f714278477218320b332d84a2a16c841",
        "install_options": "--no-build-isolation --no-cache-dir --verbose --global-option=\"build_ext\" --global-option=\"--cpp_ext\" --global-option=\"--cuda_ext\" --global-option=\"-Xcompiler\" --global-option=\"/D_ENABLE_EXTENDED_ALIGNED_STORAGE\""
      }
    ]
  },
  "files": {
    "comfy_settings": {
      "url": "https://huggingface.co/UmeAiRT/ComfyUI-Auto_installer/resolve/main/others/comfy.settings.json",
      "destination": "user/default/comfy.settings.json"
    },
    "custom_nodes_csv": {
        "url": "https://github.com/UmeAiRT/ComfyUI-Auto_installer/raw/refs/heads/main/csv/custom_nodes.csv",
        "destination": "csv/custom_nodes.csv"
    }
  }
}
